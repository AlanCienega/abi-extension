{
  "name": "abi-lenguaje",
  "displayName": "Abi Lenguaje de programacion (facilito)",
  "description": "Lenguaje para aprender a programar, sin llaves, sin cierres o aperturas feas",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.91.0"
  },
  "publisher": "AlanCienega",
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AlanCienega/abi-extension"
  },
  "icon": "images/logo.png",
  "license": "LICENSE.txt",
  "contributes": {
    "languages": [
      {
        "id": "abi",
        "aliases": [
          "abi",
          "abi"
        ],
        "extensions": [
          ".abi",
          "spl",
          "facilito"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "abi",
        "scopeName": "source.abi",
        "path": "./syntaxes/abi.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "abi",
        "path": "./snippets/abi.json"
      }
    ]
  }
}